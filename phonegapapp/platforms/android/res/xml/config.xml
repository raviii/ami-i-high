<?xml version='1.0' encoding='utf-8'?>
<widget id="org.pgapp.amihigh" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <name>AM I high</name>
    <description>
        PhoneGap 3 Version of the AmIhigh application
    </description>
    <author email="raviteja.mnnit@gmail.com" href="http://google.com">
        Teja
    </author>
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="target-device" value="universal" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="android-installLocation" value="auto" />
    <plugin name="Camera" value="org.apache.cordova.CameraLauncher" />
    <content src="index.html" />
    <access origin="*" />
</widget>
